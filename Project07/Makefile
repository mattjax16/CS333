


C_TASK_SRC = C/tasks/src/
C_TASK_BIN = C/tasks/bin/
C_EXTS_SRC = C/exts/src/
C_EXTS_BIN = C/exts/bin/



memAllocA-1: $(C_TASK_SRC)memAllocA-1.c
	
	gcc -o $(C_TASK_BIN)memAllocA-1 $(C_TASK_SRC)memAllocA-1.c $(C_TASK_SRC)class_timing.c
	./$(C_TASK_BIN)memAllocA-1


